{
  "name": "ForestWatcher",
  "version": "1.4.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:reset": "npm start -- --reset-cache",
    "start:clear": "npm run clear && npm run start:reset",
    "clear:cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && watchman watch-del-all",
    "clear:modules": "rm -rf node_modules",
    "clear": "npm run clear:cache && npm run clear:modules && npm install",
    "precommit": "lint-staged",
    "transifex:extract": "i18next-scanner --config ./app/locales/extract.config.js './app/**/*.{js,jsx}'",
    "transifex:push": "node transifex/transifex-push.js",
    "transifex:pull": "node transifex/transifex-pull.js",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "release:android": "cd android && ./gradlew assembleRelease",
    "release:ios": "react-native run-ios --configuration Release"
  },
  "dependencies": {
    "@mapbox/geo-viewport": "0.2.2",
    "@mapbox/geojson-area": "0.2.2",
    "@mapbox/tilebelt": "github:Vizzuality/tilebelt",
    "@redux-offline/redux-offline": "2.3.3",
    "appcenter": "1.4.0",
    "appcenter-crashes": "1.4.0",
    "boundingbox": "0.1.0",
    "buffer": "5.0.5",
    "d3-dsv": "1.0.5",
    "formatcoords": "1.1.3",
    "geojson-polygon-self-intersections": "1.1.1",
    "geokdbush": "1.1.0",
    "geopoint": "1.0.1",
    "jsonapi-serializer": "3.5.3",
    "kdbush": "1.0.1",
    "lodash": "4.17.4",
    "lottie-ios": "2.5.0",
    "lottie-react-native": "2.5.0",
    "moment": "2.22.0",
    "prop-types": "15.5.10",
    "react": "16.3.1",
    "react-native": "0.55.2",
    "react-native-app-auth": "2.4.0",
    "react-native-code-push": "5.3.2",
    "react-native-config": "0.11.5",
    "react-native-cookies": "3.2.0",
    "react-native-datepicker": "1.6.0",
    "react-native-fast-image": "4.0.14",
    "react-native-fbsdk": "github:Vizzuality/react-native-fbsdk",
    "react-native-fetch-blob": "0.10.8",
    "react-native-google-analytics-bridge": "5.3.0",
    "react-native-gps": "github:Vizzuality/react-native-gps",
    "react-native-hyperlink": "0.0.11",
    "react-native-i18n": "2.0.7",
    "react-native-image-picker": "0.26.7",
    "react-native-keyboard-aware-scroll-view": "0.3.0",
    "react-native-location": "github:timfpark/react-native-location",
    "react-native-maps": "github:Vizzuality/react-native-maps",
    "react-native-navigation": "1.1.434",
    "react-native-progress": "3.4.0",
    "react-native-scrollable-tab-view": "0.8.0",
    "react-native-sensor-manager": "0.1.10",
    "react-native-snap-carousel": "3.4.0",
    "react-native-timer": "1.3.1",
    "react-native-zip-archive": "2.1.0",
    "react-redux": "5.0.6",
    "realm": "1.13.0",
    "redux": "3.6.0",
    "redux-devtools-extension": "2.13.0",
    "redux-form": "7.1.1",
