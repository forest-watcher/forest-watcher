        "lottie-ios": "3.4.0",
        "lottie-react-native": "^5.1.4",
        "memoize-one": "5.1.1",
        "moment": "2.29.4",
        "prop-types": "15.8.1",
        "react": "18.2.0",
        "react-native": "0.70.0",
        "react-native-actions-sheet": "0.5.6",
        "react-native-app-auth": "6.4.3",
        "react-native-audio-recorder-player": "^3.5.3",
        "react-native-check-version": "^1.0.16",
        "react-native-compass-heading": "1.4.1",
        "react-native-config": "1.4.5",
        "react-native-datepicker": "1.7.2",
        "react-native-device-info": "^9.0.2",
        "react-native-dialogs": "1.1.1",
        "react-native-document-picker": "^8.1.3",
        "react-native-fbsdk-next": "^11.1.0",
        "react-native-fs": "2.18.0",
        "react-native-get-random-values": "1.7.0",
        "react-native-hyperlink": "0.0.19",
        "react-native-image-picker": "4.10.2",
        "react-native-keyboard-aware-scroll-view": "0.9.5",
        "react-native-keyboard-spacer": "0.4.1",
        "react-native-localize": "1.3.3",
        "react-native-mbtiles": "file:react-native-mbtiles",
        "react-native-navigation": "7.30.3",
        "react-native-permissions": "2.2.2",
        "react-native-progress": "4.1.2",
        "react-native-safe-area": "0.5.1",
        "react-native-safe-area-context": "3.3.2",
        "react-native-share": "8.1.0",
        "react-native-snap-carousel": "3.9.1",
        "react-native-svg": "11.0.1",
        "react-native-timer": "1.3.6",
        "react-native-webview": "11.26.0",
        "react-native-zip-archive": "5.0.2",
        "react-redux": "7.2.6",
        "realm": "11.3.1",
        "redux": "4.1.1",
        "redux-offline-chain": "^0.2.3",
        "redux-persist-migrate": "5.0.0",
        "redux-saga": "0.16.2",
        "redux-thunk": "2.4.0",
        "rn-fetch-blob": "0.12.0",
        "shpjs": "3.4.3",
        "tiny-emitter": "2.1.0",
