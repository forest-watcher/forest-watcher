{
  "name": "ForestWatcher",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:reset": "yarn start -- --reset-cache",
    "start:clear": "yarn run clear && yarn run start:reset",
    "clear:cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && watchman watch-del-all",
    "clear:modules": "rm -rf node_modules",
    "clear": "yarn run clear:cache && yarn run clear:modules && yarn install",
    "prepare": "patch-package",
    "transifex:extract": "i18next-scanner --config ./app/locales/extract.config.js './app/**/*.{js,jsx}'",
    "transifex:push": "node transifex/transifex-push.js",
    "transifex:pull": "node transifex/transifex-pull.js",
    "transifex:override": "node transifex/transifex-override",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "release:android": "cd android && ./gradlew assembleRelease",
    "release:ios": "react-native run-ios --configuration Release",
    "release:upstream": "git merge --squash --allow-unrelated-histories --strategy recursive -Xtheirs",
    "test": "jest --forceExit --detectOpenHandles",
    "test:ci": "jest --forceExit --detectOpenHandles --ci --coverage",
    "test:watch": "jest --watch",
    "lint": "node_modules/.bin/eslint ./app/ --max-warnings=0",
    "lint:ci": "node_modules/.bin/eslint ./app/ --max-warnings=0 -o reports/lint.html -f html",
    "flow": "flow check --show-all-errors",
    "flow:ci": "flow check --show-all-errors",
    "flow:update-lib-types": "flow-typed install --ignoreDeps dev",
    "postinstall": "yarn jetify"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "2.1.0",
    "@mapbox/geojson-area": "0.2.2",
    "@mapbox/tilebelt": "1.0.2",
    "@mauron85/react-native-background-geolocation": "0.6.3",
    "@react-native-async-storage/async-storage": "1.15.9",
    "@react-native-community/cookies": "5.0.1",
    "@react-native-community/netinfo": "6.0.4",
    "@react-native-firebase/analytics": "12.9.3",
    "@react-native-firebase/app": "12.9.3",
    "@react-native-mapbox-gl/maps": "8.3.0",
    "@react-native-picker/picker": "^2.2.1",
    "@redux-offline/redux-offline": "2.6.0-native.1",
    "@sentry/react-native": "3.1.1",
    "@tmcw/togeojson": "4.5.0",
    "@turf/bbox": "6.5.0",
    "@turf/bbox-polygon": "6.5.0",
    "@turf/helpers": "6.5.0",
    "@turf/intersect": "6.5.0",
    "@turf/kinks": "6.5.0",
    "@turf/point-to-line-distance": "6.5.0",
    "@turf/turf": "^6.5.0",
    "@turf/union": "^6.5.0",
    "buffer": "5.6.0",
    "d3-dsv": "1.2.0",
    "formatcoords": "1.1.3",
    "geokdbush": "1.1.0",
    "i18next": "19.1.0",
    "json2csv": "4.4.0",
    "jsonapi-serializer": "3.6.7",
    "jwt-decode": "^3.1.2",
    "kdbush": "1.0.1",
    "lodash": "4.17.21",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "4.1.3",
    "memoize-one": "5.1.1",
    "moment": "2.29.4",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-native": "0.66.1",
    "react-native-actions-sheet": "0.5.6",
    "react-native-app-auth": "6.4.3",
    "react-native-check-version": "^1.0.16",
    "react-native-compass-heading": "1.3.0",
    "react-native-config": "1.4.5",
    "react-native-datepicker": "1.7.2",
    "react-native-device-info": "^9.0.2",
    "react-native-dialogs": "1.1.1",
    "react-native-document-picker": "4.1.0",
    "react-native-fbsdk": "2.0.0",
    "react-native-fs": "2.18.0",
    "react-native-get-random-values": "1.7.0",
    "react-native-hyperlink": "0.0.19",
    "react-native-image-picker": "2.3.1",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-keyboard-spacer": "0.4.1",
    "react-native-localize": "1.3.3",
    "react-native-mbtiles": "file:react-native-mbtiles",
    "react-native-navigation": "7.23.1",
    "react-native-permissions": "2.2.2",
    "react-native-progress": "4.1.2",
    "react-native-safe-area": "0.5.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-share": "4.1.0",
    "react-native-snap-carousel": "3.9.1",
    "react-native-svg": "11.0.1",
    "react-native-timer": "1.3.6",
    "react-native-webview": "10.10.2",
    "react-native-zip-archive": "5.0.2",
    "react-redux": "7.2.6",
    "realm": "5.0.3",
    "redux": "4.1.1",
    "redux-offline-chain": "^0.2.3",
    "redux-persist-migrate": "5.0.0",
    "redux-saga": "0.16.2",
    "redux-thunk": "2.4.0",
    "rn-fetch-blob": "0.12.0",
    "shpjs": "3.4.3",
    "tiny-emitter": "2.1.0",
    "topojson-client": "3.1.0",
    "url-parse": "1.5.3",
    "utm-latlng": "1.0.6",
    "uuid": "8.3.2",
    "xmldom": "0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-flow": "7.14.5",
    "@babel/runtime": "^7.12.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "3.1.1",
    "dotenv": "6.1.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-native": "3.11.0",
    "faker": "^4.1.0",
    "flow-bin": "0.175.1",
    "flow-typed": "3.4.0",
    "i18next-scanner": "2.7.0",
    "jest": "^25.1.0",
    "jest-fetch-mock": "1.7.4",
    "jest-html-reporter": "2.4.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "node-fetch": "2.3.0",
    "patch-package": "6.4.7",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.15.2",
    "react-test-renderer": "17.0.2",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^2.1.4",
    "redux-mock-store": "1.5.4"
  },
  "jest": {
    "preset": "react-native",
    "coverageDirectory": "reports/coverage",
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "outputPath": "reports/test-report.html"
        }
      ]
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
